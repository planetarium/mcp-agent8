{
  "name": "scifi",
  "displayName": "Sci-Fi",
  "description": "Sci-fi theme with futuristic blue tones and sharp edges, ideal for space or tech themed games",
  "tags": ["futuristic", "tech", "space"],
  "variables": {
    "colors": {
      "background": "220 40% 10%",
      "foreground": "210 100% 90%",
      "card": "220 40% 13%",
      "card-foreground": "210 100% 90%",
      "popover": "220 40% 13%",
      "popover-foreground": "210 100% 90%",
      "primary": "195 100% 50%",
      "primary-foreground": "220 40% 10%",
      "secondary": "250 100% 60%",
      "secondary-foreground": "220 40% 10%",
      "muted": "220 40% 20%",
      "muted-foreground": "210 100% 70%",
      "accent": "280 100% 60%",
      "accent-foreground": "220 40% 10%",
      "destructive": "350 100% 60%",
      "destructive-foreground": "220 40% 10%",
      "border": "220 40% 30%",
      "ring": "195 100% 50%",
      "accent1": "280 100% 60%",
      "accent1-foreground": "220 40% 10%",
      "accent2": "160 100% 50%",
      "accent2-foreground": "220 40% 10%",
      "accent3": "30 100% 60%",
      "accent3-foreground": "220 40% 10%",
      "warning": "30 100% 50%",
      "warning-foreground": "220 40% 10%"
    },
    "fonts": {},
    "radius": "0.75rem",
    "other": {
      "input": "220 40% 30%"
    }
  },
  "styleFeatures": {
    "background": "linear-gradient(to bottom, #e11d48, #be123c)",
    "borderTopWidth": "2px",
    "borderColor": "hsla(var(--primary), 0.3)",
    "transitionProperty": "all",
    "transitionTimingFunction": "cubic-bezier(0.4, 0, 0.2, 1)",
    "transitionDuration": "150ms",
    "boxShadow": "0 0 8px rgba(225, 29, 72, 0.5)",
    "backgroundImage": "linear-gradient(to right, hsl(var(--primary)), hsl(var(--secondary)))",
    "backdropFilter": "blur(4px)",
    "borderLeftWidth": "2px",
    "borderLeftColor": "hsl(var(--primary))",
    "strokeWidth": "1.25",
    "filter": "drop-shadow(0 1px 1px rgba(0, 0, 0, 0.05))",
    "borderWidth": "1px",
    "backgroundColor": "#2a3a64",
    "border": "1px solid #9f1239",
    "borderRadius": "0.25rem",
    "padding": "0.25rem",
    "overflow": "hidden",
    "color": "white",
    "fontWeight": "500",
    "letterSpacing": "0.05em",
    "position": "absolute",
    "content": "\"\"",
    "top": "0",
    "left": "0",
    "width": "100%",
    "height": "2px",
    "textShadow": "0 1px 1px rgba(0, 0, 0, 0.5)"
  },
  "fontMappings": {
    "body": "var(--theme-font-base)",
    "headings": "var(--theme-font-heading)",
    "accent": "var(--theme-font-accent)",
    "buttons": "var(--theme-font-heading)"
  }
}
