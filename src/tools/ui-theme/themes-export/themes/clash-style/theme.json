  "styleFeatures": {
    "background": "#002060",
    "color": "white",
    "textShadow": "1px 1px 1px rgba(0, 0, 0, 0.5)",
    "border": "2px solid black",
    "boxShadow": "inset 0 0 0 2px rgba(6, 22, 55, 0.4), 0 4px 0 rgba(0, 0, 0, 0.4)",
    "fontWeight": "700",
    "position": "absolute",
    "borderRadius": "12px",
    "textTransform": "uppercase",
    "letterSpacing": "0.02em",
    "overflow": "hidden",
    "content": "\"\"",
    "top": "0",
    "left": "10%",
    "width": "80%",
    "height": "3px",
    "pointerEvents": "none",
    "transform": "translateY(2px)",
    "borderColor": "hsl(210 100% 50%)",
    "borderBottom": "2px solid black",
    "padding": "0",
    "borderTop": "2px solid black",
    "backgroundColor": "white",
    "right": "0",
    "bottom": "0",
    "display": "flex",
    "alignItems": "center",
    "justifyContent": "center",
    "zIndex": "10",
    "outline": "none",
    "clipPath": "polygon(50% 0%, 100% 25%, 100% 75%, 50% 100%, 0% 75%, 0% 25%)",
    "filter": "drop-shadow(0 1px 1px rgba(0, 0, 0, 0.5))",
    "fontSize": "1.25rem",
    "marginRight": "0.5rem",
    "gap": "0.5rem",
    "marginLeft": "8px"
  },
  "fonts": {
    "body": "Nunito, system-ui, sans-serif",
    "headings": "Lilita One, system-ui, sans-serif",
    "accent": "Bangers, system-ui, sans-serif",
    "buttons": "Lilita One, system-ui, sans-serif",
    "resources": {
      "googleFonts": [
        "https://fonts.googleapis.com/css2?family=Nunito:wght@400&display=swap",
        "https://fonts.googleapis.com/css2?family=Lilita+One:wght@400&display=swap",
        "https://fonts.googleapis.com/css2?family=Bangers:wght@400&display=swap"
      ],
      "imports": [
        "@import url('https://fonts.googleapis.com/css2?family=Nunito:wght@400&display=swap');",
        "@import url('https://fonts.googleapis.com/css2?family=Lilita+One:wght@400&display=swap');",
        "@import url('https://fonts.googleapis.com/css2?family=Bangers:wght@400&display=swap');"
      ]
