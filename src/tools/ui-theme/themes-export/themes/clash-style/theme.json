{
  "name": "clash-style",
  "displayName": "Clash Style",
  "description": "Game UI style inspired by Clash of Clans with vibrant colors and visual hierarchy",
  "tags": ["game", "colorful", "mobile"],
  ":root": {
    "background": "215 80% 20%",
    "foreground": "0 0% 100%",
    "card": "215 80% 15%",
    "card-foreground": "0 0% 100%",
    "popover": "215 80% 15%",
    "popover-foreground": "0 0% 100%",
    "primary": "210 100% 50%",
    "primary-foreground": "0 0% 100%",
    "secondary": "215 80% 25%",
    "secondary-foreground": "0 0% 100%",
    "muted": "215 80% 30%",
    "muted-foreground": "210 40% 90%",
    "accent": "40 100% 50%",
    "accent-foreground": "0 0% 0%",
    "destructive": "0 100% 50%",
    "destructive-foreground": "0 0% 100%",
    "border": "215 80% 10%",
    "input": "215 80% 30%",
    "ring": "210 100% 50%",
    "radius": "0.75rem",
    "accent1": "40 100% 50%",
    "accent1-foreground": "0 0% 0%",
    "accent2": "120 70% 50%",
    "accent2-foreground": "0 0% 0%",
    "accent3": "280 70% 60%",
    "accent3-foreground": "0 0% 100%",
    "warning": "0 100% 60%",
    "warning-foreground": "0 0% 100%"
